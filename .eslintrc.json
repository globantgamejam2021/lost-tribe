{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": "off",
        "lines-between-class-members": "off",
        "no-unused-vars": "off",
        "no-unused-vars-experimental": "error"
    },
    "settings": {
        "import/resolver": {
            "typescript": {}
        }
    }
}